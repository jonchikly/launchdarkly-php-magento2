{
    "name": "jonchikly/launchdarkly-php-magento2",
    "description": "Fork of the Official LaunchDarkly SDK for PHP modified for compatibility with Magento 2",
    "keywords": [
        "launchdarkly",
        "launchdarkly php",
        "launchdarkly magento 2"
    ],
    "homepage": "https://github.com/jonchikly/launchdarkly-php-magento2",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "LaunchDarkly <team@launchdarkly.com>",
            "homepage": "http://launchdarkly.com/"
        }
    ],
    "require": {
        "monolog/monolog": "1.16.0",
        "php": ">=5.5",
        "psr/log": "1.0.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "6.2.1",
        "kevinrob/guzzle-cache-middleware": "1.4.1",
        "phpunit/phpunit": "4.8.26",
        "phpdocumentor/phpdocumentor": "2.*",
        "predis/predis": "1.0.*",
        "zendframework/zend-serializer": "2.7.*"
    },
    "suggested": {
        "guzzlehttp/guzzle": "6.2.1",
        "kevinrob/guzzle-cache-middleware": "1.4.1",
        "monolog/monolog": "1.21.0",
        "predis/predis": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    }
}
